                    </a>
                </li>
                <!-- Products -->
                <li class="menu-item">
                    <a class="menu-link" href="/admin/products">
                        <i class="icon material-icons md-shopping_bag"></i>
                        <span class="text">Products</span>
                    </a>
                </li>
                <!-- Coupon -->
                <li class="menu-item">
                    <a class="menu-link" href="/admin/coupons">
                        <i class="icon material-icons md-card_giftcard"></i>
                        <span class="text">Create Coupon</span>
                    </a>
                </li>
                <li class="menu-item">
                    <a class="menu-link" href="/admin/managecoupon">
                        <i class="icon material-icons md-card_giftcard"></i>
                        <span class="text">Coupon Management</span>
                    </a>
                </li>
                <!-- Banner -->
                <!-- Orders -->
                <li class="menu-item">
                    <a class="menu-link" href="/admin/orders">
                        <i class="icon material-icons md-shopping_cart"></i>
                        <span class="text">Orders</span>
                    </a>
                </li>
                <li class="menu-item">
                    <a class="menu-link" href="/admin/stock">
                        <i class="icon material-icons md-shopping_cart"></i>
                        <span class="text">Stock Managment</span>
                    </a>
                </li>
                <li class="menu-item">
                    <a class="menu-link" href="/admin/salesreport">
                        <i class="icon material-icons md-add_box"></i>
                        <span class="text">Sales Report</span>
                    </a>
                </li>
